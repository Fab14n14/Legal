<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">


    <uses-permission android:name="android.permission.MODIFY_AUDIO_SETTINGS" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/entellogo_launcher"
        android:supportsRtl="true"
        android:theme="@style/Theme.PruebaAppi"
        tools:targetApi="31">
        <activity
                android:name=".MainActivity"
            android:screenOrientation="portrait"
            android:exported="true"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".vistas.Clases.Garantia_legal"
            android:screenOrientation="portrait"
            />
        <activity android:name=".vistas.Clases.Encargado_sucursal"
            android:screenOrientation="portrait"
            />
        <activity android:name=".vistas.Clases.Derechos_usuario"
            android:screenOrientation="portrait"
            />
        <activity android:name=".vistas.Clases.Reglamento_servicio"
            android:screenOrientation="portrait"
            />

    </application>

</manifest>